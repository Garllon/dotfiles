alias .='cd . && ls -al'
alias ..="cd .."
alias ...="cd ../.."

alias ls='ls -al'

alias code="cd ~/code"
alias dotfiles="cd ~/dotfiles"

alias g="git"
alias gpl="git pl"
alias gps="git push"
alias gst="git st"

alias staging="~/dotfiles/aws-switch-to.sh staging"
alias production="~/dotfiles/aws-switch-to.sh production"
