alias ..="cd .."
alias ...="cd ../.."

alias be="bundle exec"

alias code="cd ~/code"

alias g="git"
alias gpl="git pl"
alias gps="git push"
alias gst="git st"
alias gplmaster="git fetch origin && git rebase origin/master"

alias kp="cd ~/code/kp"

alias spec="rake test:prepare && rake db:migrate RAILS_ENV=test && rspec spec"
alias rbp="bundle exec rails_best_practices"
alias rubocop="bundle exec rubocop"

alias refork="git fetch upstream && git checkout master && git merge upstream/master"
