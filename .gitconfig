[user]
  name = Dennis van de Hoef
  email = dennisvdhoef@gmail.com
[alias]
  br = branch
  ci = commit
  co = checkout
  df = diff
  rb = rebase
  st = status
  sa = stash
  mt = mergetool
  cp = cherry-pick
  sm = submodule
  pl = pull --rebase
  ps = push
[push]
  default = simple
[color]
  ui = auto
  branch = auto
  diff = auto
  interactive = auto
  status = auto
  ui = true
[merge]
  tool = vimdiff
[core]
  editor = vim
  mergeoptions = "--no-edit"
  precomposeunicode = fals
 [remote "dotfiles"]                                                               
  url = git@github.com:dennisvandehoef/dotfiles.git                             
  fetch = +refs/heads/*:refs/remotes/origin/*   
